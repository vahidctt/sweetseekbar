<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="SweetSeekbarView">
        <attr name="radius" format="integer"/>
        <attr name="radiusTL" format="integer"/>
        <attr name="radiusTR" format="integer"/>
        <attr name="radiusBL" format="integer"/>
        <attr name="radiusBR" format="integer"/>
        <attr name="maxValue" format="integer"/>
        <attr name="enableBounceAnim" format="boolean"/>
        <attr name="orientation">
            <enum name="vertical" value="0"/>
            <enum name="horizontal" value="1"/>
        </attr>
        <attr name="frontTint" format="reference|color"/>
        <attr name="backTint" format="reference|color"/>
    </declare-styleable>

</resources>